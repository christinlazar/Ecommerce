    <script>
      async function removeFromWishlist(productId){
        try {
            const response = await fetch(`/removefromwishlist?id=${productId}`)
      const data = await response.json() 
      if(data.success){
        window.location.reload()
      }
        } catch (error) { 
            console.log(error)
        }
      
      }
    </script>
