    <script>
        const btn = document.getElementById('addtocartbtn')
        const outofstock = document.getElementById('outofstock')
        const errorExceeds=document.getElementById('errorExceeds')
        const qty = document.getElementById('qty')
        const size = document.getElementById('size')
        const sblock = document.getElementById('sblock')
        const mblock = document.getElementById('mblock')
        const lblock = document.getElementById('lblock')
        let ssblo = document.getElementById('ssblock')
        let mmblo = document.getElementById('mmblock')
        let llblo = document.getElementById('llblock')
        let ssblock = parseInt(document.getElementById('ssblock').innerHTML)
        let mmblock = parseInt(document.getElementById('mmblock').innerHTML)
        let llblock = parseInt(document.getElementById('llblock').innerHTML)
      
        function toLogin(){
            window.location.href="/login"
        }
       

        window.onload = function(){
            if(ssblock===0)
            {
                ssblo.style.display = "none"
                llblo.style.display = "none"
                mmblo.style.display = "none"
                mblock.style.display = 'none'
                lblock.style.display = "none"
                sblock.style.display = "none"
               outofstock.style.display = "block"
               btn.style.display="none"
            }
            else
            {
                ssblo.style.display = "block"
                llblo.style.display = "none"
                mmblo.style.display = "none"
                mblock.style.display = 'none'
                lblock.style.display = "none"
                sblock.style.display = "block"
               outofstock.style.display = "none"
            } 
        }
        function fetchSize(){
            console.log(size.value)
            
            if(size.value==="m" ){

                if(mmblock===0)
                {
                ssblo.style.display = "none"
                llblo.style.display = "none"
                mmblo.style.display = "none"
                mblock.style.display = 'none'
                lblock.style.display = "none"
                sblock.style.display = "none"
               outofstock.style.display = "block"
               btn.style.display="none"
                }
            else
            {
                ssblo.style.display = "none"
                llblo.style.display = "none"
                mmblo.style.display = "block"
                mblock.style.display = 'block'
                lblock.style.display = "none"
                sblock.style.display = "none"
               outofstock.style.display = "none"
               btn.style.display="block"
            } 
        }else if(size.value==="s" ){
            if(ssblock===0)
            {
                ssblo.style.display = "none"
                llblo.style.display = "none"
                mmblo.style.display = "none"
                mblock.style.display = 'none'
                lblock.style.display = "none"
                sblock.style.display = "none"
               outofstock.style.display = "block"
               btn.style.display="none"
            }
            else
            {
                ssblo.style.display = "block"
                llblo.style.display = "none"
                mmblo.style.display = "none"
                mblock.style.display = 'none'
                lblock.style.display = "none"
                sblock.style.display = "block"
               outofstock.style.display = "none"
               btn.style.display="block"
            } 
        }else if(size.value==="l"){
            if(llblock===0)
            {
                ssblo.style.display = "none"
                llblo.style.display = "none"
                mmblo.style.display = "none"
                mblock.style.display = 'none'
                lblock.style.display = "none"
                sblock.style.display = "none"
               outofstock.style.display = "block"
               btn.style.display="none"
            }
            else{
                ssblo.style.display = "none"
                llblo.style.display = "block"
                mmblo.style.display = "none"
                mblock.style.display = 'none'
                lblock.style.display = "block"
                sblock.style.display = "none"
               outofstock.style.display = "none"
               btn.style.display="block"
            }

        }
    }
       function addtocart(productId,quantity){
        const selectedSize = size.value
            fetch('/userproductdetail',{
                method:"post",
                headers:{
                    "Content-Type":"application/json"
               },
               body:JSON.stringify({productId,quantity,selectedSize})
            }).then(response =>response.json()).then(data=>{
                console.log(data)
                if(data.success){
                    document.getElementById('itemadded').style.display = "block";
                    setTimeout(() => {
                    window.location.reload()
                },1000);
                }else{
                  errorExceeds.textContent=data.error
                  setTimeout(() => {
                    errorExceeds.style.display="none";
                  },3000); 
                }
            }).catch(error=>{
                console.log(error.message)
            })
       }
       btn.addEventListener('click',function(){
        const qty1 = qty.innerHTML
        const productid=this.dataset.productid
        
        addtocart(productid,qty1)
       })
    </script>
