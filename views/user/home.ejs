    <!-- Vendor JS-->
    <script>
         function addToWishlist(productId){
        fetch('/addtowishlist',{
            method:"POST",
            headers:{
                "Content-Type":"application/json"
            },
            body:JSON.stringify({productId})
        }).then(response =>response.json()).then(data=>{
            if(data.success){
                window.location.reload()
            }
        })
    }

    </script>
